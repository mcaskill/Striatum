{
    "name": "brain/striatum",
    "description": "Observer pattern package and API for WordPress hooks.",
    "authors": [
        {
            "name": "Giuseppe Mazzapica",
            "email": "giuseppe.mazzapica@gmail.com",
            "homepage": "http://gm.zoomlab.it",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/mcaskill/Striatum/issues",
        "source": "https://github.com/mcaskill/Striatum"
    },
    "license": "GPL-2.0+",
    "require": {
        "php": ">=5.4",
        "brain/support": "~0.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "mockery/mockery": "dev-master",
        "brain/hooks-mock": "dev-legacy"
    },
    "suggest": {
        "brain/brain": "A simple Pimple wrapper for WordPress"
    },
    "autoload": {
        "psr-4": {
            "Brain\\Striatum\\": "src/"
        },
        "classmap": [ "Hooks.php" ]
    },
    "autoload-dev": {
        "psr-4": {
            "Brain\\Striatum\\Tests\\": "tests/src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true
    }
}
